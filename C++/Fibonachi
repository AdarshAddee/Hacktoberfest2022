#include <iostream>
using namespace std;

int main() {
    int n, num1 = 0, num2= 1, nextNum = 0;
    cout << "Enter the number: ";
    cin >> n;
    cout << "Fibonacci Series: ";
    for (int i = 1; i <= n; ++i) {
        if(i == 1) {
            cout << num1 << ", ";
            continue;
        }
        if(i == 2) {
            cout << num2<< ", ";
            continue;
        }

        nextNum = num1 + num2;
        num1 = num2 ;
        num2 = nextNum; 
        cout << nextNum << ", ";
    }
    return 0;
}
